<h1>My orders</h1>
  <% @subscriptions.each do |s| %>
  <%  car = @cars.find(s.car_id)%>
  <%= cl_image_tag car.photo.key %>

  <p>
  <strong>Model:</strong>
  <%= car.model %>
  </p>

  <p>
  <strong>Year:</strong>
  <%= car.year %>
  </p>

  <p>
  <strong>Description:</strong>
  <%= car.description %>
  </p>

  <p>
  <strong>Reg nr:</strong>
  <%= car.reg_nr %>
  </p>

  <p>
  <strong>Pick up:</strong>
  <%= s.date_start %>
  </p>

<% end %>
