<%= simple_form_for(@subscription) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :date_start %>
    <%= f.input :date_end %>
    <%= f.input :user %>
    <%= f.input :car %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
